{
  "name": "react-tool-icons",
  "version": "1.0.2",
  "description": "SVG-based React icon components",
  "keywords": [
    "React",
    "SVG",
    "Icons"
  ],
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist src/icons src/index.ts",
    "prebuild": "npm run clean",
    "build:icons": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:rollup": "rollup -c",
    "build": "npm run clean && npm run build:icons && npm run build:rollup && npm run build:types",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.json",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test && publint"
  },
  "workspaces": [
    "packages/*"
  ],
  "author": "Aditya Moharana",
  "contributors": [
    {
      "name": "Aditya Moharana",
      "url": "https://github.com/AdityaKumar41",
